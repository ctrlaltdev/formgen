      {
        "label": "Checkbox 2",
        "value": 2
      }]
    },
    {
      "label": "Radio Test",
      "type": "radio",
      "name": "radios",
      "radios":[{
        "label": "Radio 1",
        "value": 1
      },
      {
        "label": "Radio 2",
        "value": 2
      }]
    },
    {
      "label": "Select Test",
      "type": "select",
      "name": "options",
      "options":[{
        "label": "Option 1",
        "value": 1
      },
      {
        "label": "Option 2",
        "value": 2
      }]
    }
